# logrotate cacti.log
/var/log/cacti/cacti.log {
	su root logs
	olddir /var/log/archive/cacti
	rotate 7
	daily
	compress
	create 660 root http
	dateext
}

/var/log/cacti/poller.log {
	su root logs
	missingok
	olddir /var/log/archive/cacti
	rotate 7
	daily
	compress
	create 644 cacti http
	dateext
}
